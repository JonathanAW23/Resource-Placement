@{ Layout = "_LayoutAdmin"; }

<div class="row">
    <div class="col-md-6">

        <h1 class="mt-4">Data Interview</h1>
        <ol class="breadcrumb mb-4">
            <li class="breadcrumb-item active">Interview List</li>
        </ol>
    </div>

    <div class="col-md-5"></div>
        <div class="col-md-1 d-flex align-items-center">
            <div class="d-flex align-items-center">
                <a class="btn btn-secondary btn-lg" asp-controller="Interviews" asp-action="interview-form">Add</a>
            </div>
        </div>
</div>


<table class="table" id="dataJobInterview">

    <thead class="thead-dark">
        <tr>
            <th scope="col">No.</th>
            <th scope="col">Title Job</th>
            <th scope="col">Company</th>
            <th scope="col">ID Employee</th>
            <th scope="col">Employee Name</th>
            <th scope="col">Interviewer</th>
            <th scope="col">Interview Date</th>
            <th scope="col">Interview Time</th>
            <th scope="col">Status</th>
        </tr>
    </thead>

    <tbody id="JobInterview">
    </tbody>
</table>

@section Scripts {
    <script src="~/js/jobinterviewJS.js"></script>
}